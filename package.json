{
  "name": "bdit_move_etl",
  "version": "0.1.0",
  "description": "ETL scripts for MOVE, an integrated data platform for City of Toronto transportation data",
  "scripts": {
    "pre-commit:lint-staged": "lint-staged"
  },
  "devDependencies": {
    "lint-staged": "^10.4.0"
  },
  "license": "MIT",
  "gitHooks": {
    "pre-commit": "npm-run-all pre-commit:*"
  },
  "lint-staged": {
    "*.py": [
      "pylint"
    ],
    "*.sh": [
      "shellcheck --shell=bash"
    ]
  }
}
